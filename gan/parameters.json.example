{
  "epochs": 200,
  "switch_epochs": 1,

  "data_loader": {
    "batch_size": 16,
    "shuffle": false,
    "num_workers": 2
  },

  "input_image": {
    "channels": 3,
    "width": 640,
    "height": 480
  },

  "output_image": {
    "saving_path": "./saved/images/"
  },

  "generator": {
    "learning_rate": 0.0002,
    "adam_b1": 0.5,
    "adam_b2": 0.999,
    "betas_d": [0.3, 0.3, 0.3],
    "betas_b": [0.3, 0.3, 0.3],
    "b_c": 0.5,
    "saving_path": "./saved/models/generator.pt"
  },

  "discriminator": {
    "learning_rate": 0.00003,
    "adam_b1": 0.5,
    "adam_b2": 0.999,
    "input_channels": 3,
    "saving_path": "./saved/models/discriminator.pt"
  },

  "datasets": {
    "in-air": "/media/data/2022_Noya/datasets/nyu_depth_v2_labeled.mat",
    "underwater": "/media/data/2022_Noya/datasets/underwater"
  }
}
